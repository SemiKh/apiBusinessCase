<!DOCTYPE html>

<title>SousCategorie index</title>

{% block body %}
    <h1>SousCategorie index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NomSousCategorie</th>
                <th>Slug</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sous_categorie in sous_categories %}
            <tr>
                <td>{{ sous_categorie.id }}</td>
                <td>{{ sous_categorie.nomSousCategorie }}</td>
                <td>{{ sous_categorie.slug }}</td>
                <td>
                    <a href="{{ path('app_admin_sous_categorie_show', {'id': sous_categorie.id}) }}">show</a>
                    <a href="{{ path('app_admin_sous_categorie_edit', {'id': sous_categorie.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_admin_sous_categorie_new') }}">Create new</a>
{% endblock %}
