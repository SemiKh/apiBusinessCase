<!DOCTYPE html>

<title>Transaction</title>

{% block body %}
    <h1>Transaction</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transaction.id }}</td>
            </tr>
            <tr>
                <th>DateAcquisition</th>
                <td>{{ transaction.dateAcquisition ? transaction.dateAcquisition|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ transaction.slug }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_admin_transaction_index') }}">back to list</a>

    <a href="{{ path('app_admin_transaction_edit', {'id': transaction.id}) }}">edit</a>

    {{ include('transaction/_delete_form.html.twig') }}
{% endblock %}
